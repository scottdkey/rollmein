{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/server/index.js",
    "test": "./node_modules/mocha/bin/_mocha --exit",
    "reset_db": "run-s reset_prod-db reset_test-db reset_dev-db",
    "reset_prod-db": "run-s del-prod create-prod",
    "reset_test-db": "run-s del-test create-test",
    "reset_dev-db": "run-s del-dev create-dev",
    "del-dev": "sudo -u postgres psql -f ./src/db/sql/delete_dev.sql",
    "del-prod": "sudo -u postgres psql -f ./src/db/sql/delete_prod.sql",
    "del-test": "sudo -u postgres psql -f ./src/db/sql/delete_test.sql",
    "create-test": "sudo -u postgres psql -f ./src/db/sql/create_test.sql",
    "create-prod": "sudo -u postgres psql -f ./src/db/sql/create_prod.sql",
    "create-dev": "sudo -u postgres psql -f ./src/db/sql/create_dev.sql",
    "migrate": "knex migrate:latest",
    "seed_user": "knex seed:run --specific=users.js",
    "seed_players": "knex seed:run --specific=players.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "knex": "^0.21.5",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^9.4.0",
    "koa-session": "^6.0.0",
    "nodemon": "^2.0.4",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "sinon": "^9.0.3"
  }
}
