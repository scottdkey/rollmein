{
  "name": "rollmein_websocket",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "type": "module",
  "author": "Scott Key",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/websocket/src/index.js",
    "dev": "run-p watch:*",
    "test": "mocha -r ts-node/register src/tests/**/*.test.ts --exit",
    "lint": "eslint",
    "build": "tsc",
    "watch:ts": "tsc -w",
    "watch:node": "nodemon ./dist/websocket/src/index.js | npx pino-pretty",
    "docker:build": "docker build -t scottdkey/rollmein_websocket:$(echo $npm_package_version) -f Dockerfile ../ --no-cache && docker tag scottdkey/rollmein_api:$(echo $npm_package_version) scottdkey/rollmein_api:latest",
    "docker:push": "docker push scottdkey/rollmein_websocket:$(echo $npm_package_version) && docker push scottdkey/rollmein_api:latest"
  },
  "dependencies": {
    "argon2": "0.31.2",
    "chalk": "5.3.0",
    "class-validator": "0.14.0",
    "cookies": "0.8.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.1.0",
    "inversify": "6.0.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "9.0.2",
    "pg": "8.11.3",
    "pino": "8.17.1",
    "pino-pretty": "10.3.0",
    "redis": "^4.6.11",
    "reflect-metadata": "0.2.1",
    "socket.io": "4.7.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.11",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.4",
    "@types/pg": "8.10.9",
    "@types/superagent": "4.1.24",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "chai": "4.3.10",
    "chai-http": "4.4.0",
    "eslint": "8.56.0",
    "mocha": "10.2.0",
    "nodemon": "3.0.2",
    "npm-run-all": "4.1.5",
    "request": "2.88.2",
    "should": "13.2.3",
    "sinon": "17.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
