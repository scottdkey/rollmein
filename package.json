{
  "license": "MIT",
  "scripts": {
    "dev": "docker-compose up -d && yarn migrate up",
    "dev:down": "docker-compose down",
    "dev:clean": "docker-compose down -v",
    "prod": "docker-compose -f prod.docker-compose.yml pull && yarn migrate up && docker-compose -f prod.docker-compose.yml up -d",
    "prod:down": "docker-compose -f prod.docker-compose.yml down",
    "prod:clean": "docker-compose -f prod.docker-compose.yml down -v",
    "migrate": "node node_modules/db-migrate/bin/db-migrate -m ./migrations -e pg"
  },
  "dependencies": {
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2"
  }
}
