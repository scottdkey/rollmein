{
  "name": "rollmein",
  "version": "1.0.0",
  "description": "Web and React native app to choose who is going on the mythic + dungeon from a guild or custom list",
  "main": "index.js",
  "scripts": {
    "start": "run-p start_web start_api",
    "start_web": "cd web && yarn start",
    "start_api": "cd api && npm start",
    "reset_db": "run-s setup_db seed_db",
    "setup_db": "cd api && npm run reset_db",
    "seed_db": "cd api && npm run seed_db",
    "install_dev": "run-s install_api_deps install_web_deps",
    "install_api_deps": "cd api && npm i",
    "install_web_deps": "cd web && npm i"
  },
  "author": "Scott Key <scottdkey@gmail.com> (www.github.com/scottdkey)",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-tabs": "^2.3.2",
    "knex": "^0.21.5",
    "prompt-confirm": "^2.0.4",
    "react-hook-form": "^6.7.2",
    "react-hooks-use-modal": "^1.0.9",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-tabs": "^3.1.1",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
